bpp	show_png_image.c	/^static unsigned int bpp;    \/\/像素深度bpp$/;"	v	file:
buf	setjmp.c	/^static jmp_buf buf;$/;"	v	file:
height	show_png_image.c	/^static int height;                      \/\/LCD Y分辨率$/;"	v	file:
hello	setjmp.c	/^static void hello(void)$/;"	f	file:
line_length	show_png_image.c	/^static unsigned long line_length;       \/\/LCD一行的长度（字节为单位）$/;"	v	file:
main	setjmp.c	/^int main(void)$/;"	f
main	show_png_image.c	/^int main(int argc, char *argv[])$/;"	f
screen_base	show_png_image.c	/^static unsigned short *screen_base = NULL;        \/\/映射后的显存基地址$/;"	v	file:
show_png_image	show_png_image.c	/^static int show_png_image(const char *path)$/;"	f	file:
width	show_png_image.c	/^static int width;                       \/\/LCD X分辨率$/;"	v	file:
